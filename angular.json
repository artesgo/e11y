{
  "version": 1,
  "projects": {
    "elly": {
      "projectType": "application",
      "schematics": {
        "@nrwl/angular:component": {
          "style": "scss"
        }
      },
      "root": "apps/elly",
      "sourceRoot": "apps/elly/src",
      "prefix": "elly",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/apps/elly",
            "index": "apps/elly/src/index.html",
            "main": "apps/elly/src/main.ts",
            "polyfills": "apps/elly/src/polyfills.ts",
            "tsConfig": "apps/elly/tsconfig.app.json",
            "aot": true,
            "assets": ["apps/elly/src/favicon.ico", "apps/elly/src/assets"],
            "styles": ["apps/elly/src/styles.scss"],
            "scripts": []
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/elly/src/environments/environment.ts",
                  "with": "apps/elly/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "elly:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "elly:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "elly:build"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "apps/elly/tsconfig.app.json",
              "apps/elly/tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**", "!apps/elly/**"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "apps/elly/jest.config.js",
            "tsConfig": "apps/elly/tsconfig.spec.json",
            "passWithNoTests": true,
            "setupFile": "apps/elly/src/test-setup.ts"
          }
        }
      }
    },
    "elly-e2e": {
      "root": "apps/elly-e2e",
      "sourceRoot": "apps/elly-e2e/src",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/elly-e2e/cypress.json",
            "tsConfig": "apps/elly-e2e/tsconfig.e2e.json",
            "devServerTarget": "elly:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "elly:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": ["apps/elly-e2e/tsconfig.e2e.json"],
            "exclude": ["**/node_modules/**", "!apps/elly-e2e/**"]
          }
        }
      }
    },
    "elly-lib": {
      "projectType": "library",
      "root": "libs/elly",
      "sourceRoot": "libs/elly/src",
      "prefix": "elly",
      "architect": {
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "libs/elly/tsconfig.lib.json",
              "libs/elly/tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**", "!libs/elly/**"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "libs/elly/jest.config.js",
            "tsConfig": "libs/elly/tsconfig.spec.json",
            "passWithNoTests": true,
            "setupFile": "libs/elly/src/test-setup.ts"
          }
        }
      },
      "schematics": {
        "@nrwl/angular:component": {
          "styleext": "scss"
        }
      }
    }
  },
  "cli": {
    "defaultCollection": "@nrwl/angular",
    "analytics": "4101f7e0-4a70-4fd2-b3f6-c790e8e70bb4"
  },
  "schematics": {
    "@nrwl/angular:application": {
      "unitTestRunner": "jest",
      "e2eTestRunner": "cypress"
    },
    "@nrwl/angular:library": {
      "unitTestRunner": "jest"
    },
    "@nrwl/react": {
      "application": {
        "babel": true
      }
    }
  },
  "defaultProject": "elly"
}
